	return 0;
}

switch(FEAT_TRAINS, PARENT, sw_BR350_capacity_62_with_translator, (bitmask_consist_info & 0x01)) {
	0: return (62 * param_pax);
	return 0;
}

switch(FEAT_TRAINS, PARENT, sw_BR350_capacity_60_with_translator, (bitmask_consist_info & 0x01)) {
	0: return (60 * param_pax);
	return 0;
}

// Mute Class 350 custom sounds when a translator wagon is present in the consist.
switch(FEAT_TRAINS, SELF, sw_sound_electric_350_muted, getbits(extra_callback_info1, 0, 8)) {
	SOUND_EVENT_BREAKDOWN: SOUND_BREAKDOWN_TRAIN_SHIP;
}

switch(FEAT_TRAINS, PARENT, sw_sound_electric_350_with_translator, (bitmask_consist_info & 0x01)) {
	0: sw_sound_electric_350;
	sw_sound_electric_350_muted;
}

// Disable Class 350 electric visual effects when a translator wagon is present.
switch(FEAT_TRAINS, PARENT, sw_BR350_visual_effect_with_translator, (bitmask_consist_info & 0x01)) {
	0: visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 2, DISABLE_WAGON_POWER);
	visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
}

// Class 350/1: choose visual effect by available power source.
// If both are present, overhead takes priority (pantograph sparks).
switch(FEAT_TRAINS, PARENT, sw_BR350_1_visual_effect_by_railtype_third, tile_powers_railtype("THIRD")) {
	1: visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 2, DISABLE_WAGON_POWER);
	visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
