}

switch(FEAT_TRAINS, PARENT, sw_BR350_1_visual_effect_with_translator, (bitmask_consist_info & 0x01)) {
	0: sw_BR350_1_visual_effect_by_railtype;
	visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
}

